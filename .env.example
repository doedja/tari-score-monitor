# Tari Score Monitor Environment Variables

# Database configuration
DB_PATH=./tari_monitor.db

# Discord notification configuration
DISCORD_NOTIFICATION_INTERVAL=1800 # Seconds between discord notifications

# Score fetch configuration
FETCH_INTERVAL_MIN=240 # Minimum seconds between fetches (default 4 min)
FETCH_INTERVAL_MAX=300 # Maximum seconds between fetches (default 5 min)

# Admin authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin